        android:layout_height="50dp"
        android:layout_marginTop="16dp"
        android:contentDescription="@string/paket"
        app:layout_constraintStart_toEndOf="@+id/img_counter_1lei"
        app:layout_constraintTop_toBottomOf="@+id/p50_plus"
