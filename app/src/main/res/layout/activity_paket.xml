        tools:srcCompat="@drawable/paket1432" />

    <ImageButton
        android:id="@+id/p14_32_plus"
        android:layout_width="60dp"
        android:layout_height="50dp"
        android:layout_marginTop="16dp"
        android:contentDescription="@string/paket"
        app:layout_constraintStart_toEndOf="@+id/img_counter14_32"
        app:layout_constraintTop_toTopOf="parent"
